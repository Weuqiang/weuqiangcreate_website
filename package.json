    "@docusaurus/remark-plugin-npm2yarn": "^3.9.2",
    "@docusaurus/theme-live-codeblock": "^3.9.2",
    "@docusaurus/theme-mermaid": "^3.9.2",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@giscus/react": "^2.4.0",
    "@infinum/docusaurus-theme": "^0.4.0",
    "@mdx-js/react": "^3.0.0",
    "@mui/material": "^5.15.1",
    "clsx": "^2.0.0",
    "d3": "^7.9.0",
    "markmap-common": "^0.15.6",
    "markmap-lib": "^0.15.8",
    "markmap-toolbar": "^0.15.8",
    "markmap-view": "^0.15.8",
