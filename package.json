  "description": "个人知识库 - 涵盖前端开发、人工智能、编程语言等技术领域",
  "keywords": [
    "前端开发",
    "JavaScript",
    "React",
    "Vue",
    "人工智能",
    "机器学习",
    "编程教程",
    "技术博客"
  ],
  "author": "魏强 <wxai2411@example.com>",
  "homepage": "https://weuqiang.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/Weuqiang/weuqiangcreate_website.git"
  },
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "dev": "docusaurus start --host 0.0.0.0",
    "build": "docusaurus build",
    "build:analyze": "npm run build -- --bundle-analyzer",
    "swizzle": "docusaurus swizzle",
    "deploy": "GIT_USER=Weuqiang docusaurus deploy",
    "deploy:github": "npm run build && gh-pages -d build",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "serve:build": "npm run build && npm run serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md,mdx}\""
  },
